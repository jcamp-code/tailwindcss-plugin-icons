{
  "name": "@jcamp/tailwindcss-plugin-icons",
  "version": "0.4.0",
  "description": "Automatic icon creator for Tailwind",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc --watch & cd ./demo && npm run-script dev",
    "build": "tsc",
    "release": "np",
    "test": "tsc && c8 jest"
  },
  "license": "MIT",
  "author": "John Campion (https://github.com/JohnCampionJr/)",
  "repository": "https://github.com/jcamp-code/tailwindcss-plugin-icons",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@iconify/types": "^1.1.0",
    "c8": "^7.12.0",
    "jest": "^29.0.1",
    "np": "^7.6.2",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.0",
    "typescript": "4.8.2"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.1.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest/customMatchers.js"
    ]
  },
  "c8": {
    "exclude": [
      "jest/customMatchers.js",
      "**/*.test.js"
    ]
  },
  "keywords": [
    "tailwindcss",
    "iconify",
    "icons"
  ],
  "dependencies": {
    "@iconify/json": "2.2.7",
    "@iconify/utils": "^1.0.33"
  }
}
